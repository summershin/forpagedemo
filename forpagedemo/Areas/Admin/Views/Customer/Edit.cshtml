
@{
    ViewData["Title"] = "Edit";
}
<br />
<h1 class="text-center">修改會員資料</h1>
<br />
<div class=container-fluid pt-4 px-4">
    <div class="row g-4">
        <div class="col-sm-12 col-xl-12">       
            <!--============================================這邊加入其他頁面======================================================================================-->
            <div class="bg-secondary shadow" style="padding: 35px;">
                <div class="tab-content">
                    @*// HTML 死資料 //*@

                    <div class="container px-4">
                        <div class="row gx-5">
                            <div class="col">
                                <div class="p-3 border bg-secondary">
                                    <label for="exampleFormControlInput1" class="form-label text-white"> 姓氏 </label>
                                    <br />
                                    <input type="text" class="text-primary bg-dark" id="FLastname" placeholder="戴樂古">
                                </div>
                            </div>
                            <div class="col">
                                <div class="p-3 border bg-secondary">
                                    <label for="exampleFormControlInput1" class="form-label text-white"> 名字 </label>
                                    <br />
                                    <input type="text" class="text-primary bg-dark" id="FFristname" placeholder="花兒">
                                </div>
                            </div>
                            <div class="col">
                                <div class="p-3 border bg-secondary">
                                    <label for="exampleFormControlInput1" class="form-label text-white"> 性別 </label>
                                    <br />
                                    <input type="text" class="text-primary bg-dark" id="FGender" placeholder="女">
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="container px-4">
                        <div class="row gx-5">
                            <div class="col">
                                <div class="p-3 border bg-secondary">
                                    <label for="exampleFormControlInput1" class="form-label text-white"> 居住城市 </label>
                                    <br />
                                    <input type="text" class="text-primary bg-dark" id="FCityId" placeholder="臺北市">
                                </div>
                            </div>
                            <div class="col">
                                <div class="p-3 border bg-secondary">
                                    <label for="exampleFormControlInput1" class="form-label text-white"> 生日 </label>
                                    <br />
                                    <input type="text" class="text-primary bg-dark" id="FBirth" placeholder="1990-10-27">
                                </div>
                            </div>
                            <div class="col">
                                <div class="p-3 border bg-secondary">
                                    <label for="exampleFormControlInput1" class="form-label text-white"> 手機號碼 </label>
                                    <br />
                                    <input type="text" class="text-primary bg-dark" id="FPone" placeholder="0931000013">
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="container px-4">
                        <div class="row gx-5">
                            <div class="col">
                                <div class="p-3 border bg-secondary">
                                    <label for="exampleFormControlInput1" class="form-label text-white"> 通訊地址 </label>
                                    <br />
                                    <input type="text" class="form-control text-primary" id="FAddress" placeholder="106 臺北市大安區安東街15號">
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="container px-4">
                        <div class="row gx-5">
                            <div class="col">
                                <div class="p-3 border bg-secondary">
                                    <label class="form-label text-white"> E-Mail </label>
                                    <br />
                                    <input type="text" class="form-control text-white" id="FEmail" placeholder="FleurIsabellaDelacour@gmail.com" aria-label="default input example">
                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="d-grid gap-2 d-md-flex justify-content-md-end">
                        <input type="submit" value="儲存並修改" class="btn btn-primary " />
                    </div>
                </div>
            </div>
            <!--============================================這邊加入其他頁面=========================================================================END=======-->
        </div>
        </div>
    </div>

    
